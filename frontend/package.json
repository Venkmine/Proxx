{
  "name": "proxx-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist-electron/main.mjs",
  "scripts": {
    "dev": "vite",
    "dev:electron": "vite & sleep 3 && pnpm run electron:dev",
    "build": "tsc && vite build && electron-builder",
    "preview": "vite preview",
    "electron:dev": "tsc -p tsconfig.electron.json && sh ./rename-electron-js-to-mjs.sh && VITE_DEV_SERVER_URL=http://localhost:5173 electron dist-electron/main.mjs",
    "electron:build": "tsc -p tsconfig.electron.json && sh ./rename-electron-js-to-mjs.sh",
    "pnpm:electron:dev": "pnpm --filter ./frontend electron:dev"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "framer-motion": "^11.11.11"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "electron": "^33.2.0",
    "electron-builder": "^25.1.8",
    "typescript": "^5.7.2",
    "vite": "^6.0.3",
    "vite-plugin-electron": "^0.28.7"
  }
}
