{
  "name": "awaire-proxy",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "dist-electron/main.mjs",
  "scripts": {
    "dev": "vite",
    "dev:electron": "vite & sleep 3 && pnpm run electron:dev",
    "build": "tsc && vite build && electron-builder",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "cd ../qa/e2e && E2E_TEST=true playwright test",
    "test:e2e:ui": "cd ../qa/e2e && E2E_TEST=true playwright test --ui",
    "test:e2e:debug": "cd ../qa/e2e && E2E_TEST=true playwright test --debug",
    "test:e2e:truth": "cd ../qa/verify/ui/audit_truth_surface && E2E_TEST=true E2E_AUDIT_MODE=0 playwright test",
    "test:e2e:audit": "cd ../qa/verify/ui/audit_internal_mode && E2E_TEST=true E2E_AUDIT_MODE=1 playwright test",
    "test:e2e:report": "node ../scripts/build_e2e_audit_report.mjs",
    "qc:visual": "node ../scripts/qc/run_visual_qc.mjs",
    "qc:visual:glm": "node ../scripts/qc/run_glm_visual_judge.mjs",
    "qc:interpret": "node ../scripts/qc/interpret_glm_report.mjs",
    "qc:loop": "node ../scripts/qc/run_qc_loop.mjs",
    "qc:diff": "node ../scripts/qc/diff_glm_reports.mjs",
    "electron:dev": "tsc -p tsconfig.electron.json && sh ./rename-electron-js-to-mjs.sh && VITE_DEV_SERVER_URL=http://localhost:5173 electron dist-electron/main.mjs",
    "electron:build": "tsc -p tsconfig.electron.json && sh ./rename-electron-js-to-mjs.sh",
    "pnpm:electron:dev": "pnpm --filter ./frontend electron:dev"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "framer-motion": "^11.11.11",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/coverage-v8": "^4.0.16",
    "electron": "^33.2.0",
    "electron-builder": "^25.1.8",
    "playwright": "^1.57.0",
    "typescript": "^5.7.2",
    "vite": "^6.0.3",
    "vite-plugin-electron": "^0.28.7",
    "vitest": "^4.0.16"
  }
}
